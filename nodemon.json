{
    "restartable": "rs",
    "ignore": [
        ".git",
        "node_modules/**/node_modules",
        "doc",
        "migrations",
        "dist"
    ],
    "verbose": true,
    "execMap": {
      "ts": "node --require ts-node/register --require dotenv/config"
    },
    "watch": ["src/"],
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "js,json,ts"
}
  